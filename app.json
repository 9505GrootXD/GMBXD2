{
  "name": "𝗚𝗿𝗼𝗼𝘁 𝗠𝘂𝘀𝗶𝗰 𝗕𝗼𝘁",
  "logo": "https://graph.org/file/ae44ceeed0e39f91e81f8.jpg",
  "description": "🥀A Bot For Playing High Quality and Unbreakable Music in Your Groups Voice Chat",
  "keywords": ["music", "voicechat", "telegram"],
  "repository": "https://github.com/9505GrootXD/GMBXD2",
  "stack": "container",
  "env": {
    "SESSION_NAME": {
      "description": "Pyrogram session string",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "A bot token from @BotFather",
      "required": true
    },
    "BOT_NAME": {
      "description": "Your MusicPlayer Bot Name.",
      "required": false,
      "value": "Leeyoo_Robot"
    },
    "API_ID": {
      "description": "App ID from my.telegram.org/apps",
      "required": true
    },
    "API_HASH": {
      "description": "App hash from my.telegram.org/apps",
      "required": true
    },
    "SUDO_USERS": {
      "description": "List of user IDs counted as admin everywhere (separated by space).",
      "required": true,
      "value": "6285755686"
    },
    "DURATION_LIMIT": {
      "description": "Max audio duration limit for downloads (minutes).",
      "required": true,
      "value": "120"
    }
  }
}
